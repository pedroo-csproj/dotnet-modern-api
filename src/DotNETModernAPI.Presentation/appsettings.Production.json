{
  "Environment": "Production",
  "BuildId": "#{Build.BuildId}#",
  "ConnectionStrings": {
    "PostgreSQL": "#{SQLDBConnectionString}#"
  },
  "JWT": {
    "ValidIssuer": "#{JWTValidIssuer}#",
    "ValidAudience": "#{JWTValidAudience}#",
    "Secret": "#{JWTSecret}#"
  },
  "Email": {
    "UserName": "#{EmailUserName}#",
    "From": "#{EmailFrom}#",
    "Password": "#{EmailPassword}#",
    "Host": "#{EmailHost}#",
    "Port": "#{EmailPort}#"
  },
  "Policies": {
    "Users": [
      "users.list",
      "users.register"
    ],
    "Roles": [
      "roles.listPolicies",
      "roles.create",
      "roles.addClaimsToRole"
    ]
  }
}
